#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/radrat/workspace/radarsu-pnpm/@package/@/cli/dist/bin/bin.js/node_modules:/home/radrat/workspace/radarsu-pnpm/@package/@/cli/dist/bin/node_modules:/home/radrat/workspace/radarsu-pnpm/@package/@/cli/dist/node_modules:/home/radrat/workspace/radarsu-pnpm/@package/@/cli/node_modules:/home/radrat/workspace/radarsu-pnpm/@package/@/node_modules:/home/radrat/workspace/radarsu-pnpm/@package/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules:/home/radrat/workspace/node_modules:/home/radrat/node_modules:/home/node_modules:/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/@radrat/cli/dist/bin/bin.js/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/@radrat/cli/dist/bin/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/@radrat/cli/dist/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/@radrat/cli/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/@radrat/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/node_modules:/home/radrat/workspace/radarsu-pnpm/@/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@radrat/cli/dist/bin/bin.js" "$@"
else 
  exec node  "$basedir/../@radrat/cli/dist/bin/bin.js" "$@"
fi
