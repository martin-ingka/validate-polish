#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/radrat/workspace/radarsu-pnpm/node_modules/.pnpm/ts-jest@26.3.0_jest@26.4.2+typescript@4.0.2/node_modules/ts-jest/cli.js/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules/.pnpm/ts-jest@26.3.0_jest@26.4.2+typescript@4.0.2/node_modules/ts-jest/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules/.pnpm/ts-jest@26.3.0_jest@26.4.2+typescript@4.0.2/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules/.pnpm/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules:/home/radrat/workspace/node_modules:/home/radrat/node_modules:/home/node_modules:/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/ts-jest/cli.js/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/ts-jest/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/node_modules:/home/radrat/workspace/radarsu-pnpm/@/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else 
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
