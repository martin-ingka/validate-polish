#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/radrat/workspace/radarsu-pnpm/node_modules/.pnpm/typescript@4.0.2/node_modules/typescript/bin/tsserver/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules/.pnpm/typescript@4.0.2/node_modules/typescript/bin/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules/.pnpm/typescript@4.0.2/node_modules/typescript/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules/.pnpm/typescript@4.0.2/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules/.pnpm/node_modules:/home/radrat/workspace/radarsu-pnpm/node_modules:/home/radrat/workspace/node_modules:/home/radrat/node_modules:/home/node_modules:/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/typescript/bin/tsserver/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/typescript/bin/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules/typescript/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/validate-polish/node_modules:/home/radrat/workspace/radarsu-pnpm/@/any/node_modules:/home/radrat/workspace/radarsu-pnpm/@/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else 
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
